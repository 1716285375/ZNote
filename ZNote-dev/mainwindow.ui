/* =========================================
   QComboBox 完整样式模板
========================================= */

/* QComboBox 容器 */
QComboBox#cmbThreads {
    padding: 4px 8px;                  /* 内边距 */
    background-color: #1F2937;         /* 背景色 */
    border: 1px solid #4B5563;         /* 默认边框 */
    border-radius: 6px;                /* 圆角 */
    color: #F3F4F6;                    /* 文字颜色 */
    font-size: 13px;                    /* 字体大小 */
    min-width: 180px;                   /* 最小宽度 */
}

/* QComboBox 获得焦点 */
QComboBox#cmbThreads:focus {
    border: 1px solid #3B82F6;         /* 焦点边框 */
}

/* 下拉箭头容器 */
QComboBox#cmbThreads::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: none;
}

/* 下拉箭头图标 */
QComboBox#cmbThreads::down-arrow {
    image: url(:/icons/down-arrow.png); /* 替换为你的箭头图标路径 */
    width: 12px;
    height: 12px;
}

/* 下拉列表 */
QComboBox#cmbThreads QAbstractItemView {
    background-color: #1F2937;         /* 列表背景色 */
    color: #F3F4F6;                    /* 列表文字 */
    border: 1px solid #4B5563;         /* 列表边框 */
    outline: 0;                         /* 去掉默认焦点框 */
}

/* 鼠标悬停项高亮 */
QComboBox#cmbThreads QAbstractItemView::item:hover {
    background-color: #2563EB;         /* 悬停颜色 */
    color: #FFFFFF;
}

/* 当前选中项背景 */
QComboBox#cmbThreads QAbstractItemView::item:selected {
    background-color: #3B82F6;         /* 当前选中项背景 */
    color: #FFFFFF;                     /* 文字颜色 */
}


