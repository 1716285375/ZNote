/* ==============================================
   全局按钮样式统一（含按压动态效果）
   适用按钮：btnResolve/btnDownloadList/btnDownloadStatus/btnNote/btnVideo/btnSetting/btnAbout
   ============================================== */

/* ==============================================
   全局按钮样式统一（含按压动态效果）
   适用按钮：btnResolve/btnDownloadList/btnDownloadStatus/btnNote/btnVideo/btnSetting/btnAbout
   ============================================== */

/* 1. 基础状态（所有按钮通用） */
QPushButton#btnResolve,
QPushButton#btnDownloadList,
QPushButton#btnDownloadStatus,
QPushButton#btnNote,
QPushButton#btnVideo,
QPushButton#btnSetting,
QPushButton#btnAbout {
    /* 基础布局属性 */
    padding: 6px 12px; /* 统一内边距，优化点击区域 */
    border-radius: 8px; /* 统一圆角，保持视觉一致性 */
    border: none; /* 取消默认边框 */
    color: white; /* 统一文字颜色 */
    font-weight: 500; /* 文字加粗，提升辨识度 */
    
    /* 主题色渐变（默认未选中态） */
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                                stop:0 #3B82F6,  /* 主色-浅 */
                                stop:1 #2563EB); /* 主色-深 */


}


/* 2. 交互状态 - 悬停（所有按钮通用） */
QPushButton#btnResolve:hover,
QPushButton#btnDownloadList:hover,
QPushButton#btnDownloadStatus:hover,
QPushButton#btnNote:hover,
QPushButton#btnVideo:hover,
QPushButton#btnSetting:hover,
QPushButton#btnAbout:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                                stop:0 #93C5FD,  /* 主色-超浅 */
                                stop:1 #3B82F6); /* 主色-浅 */
}


/* 3. 交互状态 - 按压（核心动态效果，所有按钮通用） */
QPushButton#btnResolve:pressed,
QPushButton#btnDownloadList:pressed,
QPushButton#btnDownloadStatus:pressed,
QPushButton#btnNote:pressed,
QPushButton#btnVideo:pressed,
QPushButton#btnSetting:pressed,
QPushButton#btnAbout:pressed {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                                stop:0 #1D4ED8,  /* 主色-更深 */
                                stop:1 #1E40AF); /* 主色-最深 */
}


/* 4. 功能状态 - 选中（所有按钮通用） */
QPushButton#btnResolve:checked,
QPushButton#btnDownloadList:checked,
QPushButton#btnDownloadStatus:checked,
QPushButton#btnNote:checked,
QPushButton#btnVideo:checked,
QPushButton#btnSetting:checked,
QPushButton#btnAbout:checked {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                                stop:0 #10B981,  /* 成功色-浅 */
                                stop:1 #059669); /* 成功色-深 */
}


/* 5. 组合状态 - 选中+悬停（所有按钮通用） */
QPushButton#btnResolve:checked:hover,
QPushButton#btnDownloadList:checked:hover,
QPushButton#btnDownloadStatus:checked:hover,
QPushButton#btnNote:checked:hover,
QPushButton#btnVideo:checked:hover,
QPushButton#btnSetting:checked:hover,
QPushButton#btnAbout:checked:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                                stop:0 #34D399,  /* 成功色-超浅 */
                                stop:1 #10B981); /* 成功色-浅 */
}


/* 6. 组合状态 - 选中+按压（所有按钮通用） */
QPushButton#btnResolve:checked:pressed,
QPushButton#btnDownloadList:checked:pressed,
QPushButton#btnDownloadStatus:checked:pressed,
QPushButton#btnNote:checked:pressed,
QPushButton#btnVideo:checked:pressed,
QPushButton#btnSetting:checked:pressed,
QPushButton#btnAbout:checked:pressed {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                                stop:0 #059669,  /* 成功色-深 */
                                stop:1 #065F46); /* 成功色-最深 */
}


/* 7. 功能状态 - 禁用（所有按钮通用） */
QPushButton#btnResolve:disabled,
QPushButton#btnDownloadList:disabled,
QPushButton#btnDownloadStatus:disabled,
QPushButton#btnNote:disabled,
QPushButton#btnVideo:disabled,
QPushButton#btnSetting:disabled,
QPushButton#btnAbout:disabled {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                                stop:0 #4B5563,  /* 灰色-中 */
                                stop:1 #374151); /* 灰色-深 */
    opacity: 0.7; /* 降低透明度，明确禁用状态 */
}

