/* ================== iOS 风格滑块 ================== */
QCheckBox#chkOpenDir,
QCheckBox#chkPlaySound {
    spacing: 8px;  /* 文字和滑块间距 */
    font-size: 13px;
    color: #F3F4F6;
}

/* 隐藏默认方框，滑块容器 */
QCheckBox#chkOpenDir::indicator,
QCheckBox#chkPlaySound::indicator {
    width: 40px;
    height: 20px;
    border-radius: 10px;
    background-color: #ccc; /* 默认背景灰色 */
    position: relative;
    transition: background 0.2s;
}

/* 滑块圆点 */
QCheckBox#chkOpenDir::indicator:before,
QCheckBox#chkPlaySound::indicator:before {
    content: '';
    position: absolute;
    top: 2px;
    left: 2px;
    width: 16px;
    height: 16px;
    border-radius: 8px;
    background: white;
    transition: left 0.2s;
}

/* 选中状态背景颜色 */
QCheckBox#chkOpenDir::indicator:checked,
QCheckBox#chkPlaySound::indicator:checked {
    background-color: #3B82F6; /* 蓝色背景 */
}

/* 滑块圆点移动 */
QCheckBox#chkOpenDir::indicator:checked:before,
QCheckBox#chkPlaySound::indicator:checked:before {
    left: 22px; /* 滑块滑动到右边 */
}

