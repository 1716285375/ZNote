}



/* QComboBox 样式 */
QComboBox#cboResolution {
    background-color: #374151; /* var(--gray-700) */
    color: #F3F4F6; /* var(--gray-100) */
    border: 1px solid #4B5563; /* var(--gray-600) */
    border-radius: 6px;
    padding: 6px 30px 6px 12px;
}

QComboBox#cboResolution:hover {
    border-color: #6B7280; /* var(--gray-500) */
}

QComboBox#cboResolution:focus {
    border-color: #3B82F6; /* var(--primary) */
}

/* 下拉按钮区域样式 */
QComboBox#cboResolution::drop-down {
    subcontrol-origin: content; /* 改为content可能解决位置问题 */
    subcontrol-position: top right;
    width: 24px; /* 足够容纳箭头 */
    height: 100%; /* 占满整个控件高度 */
    border-left: 1px solid #4B5563;
    background-color: transparent; /* 继承父控件背景 */
}

/* 下拉箭头样式 - 关键修正 */
QComboBox#cboResolution::down-arrow {
    /* 移除中间的::drop-down，Qt选择器不支持这种嵌套 */
    image: url(&quot;qrc:/icons/down-arrow.png&quot;);
    width: 16px;
    height: 16px;
    /* 添加偏移调整位置 */
    margin: 0 4px;
}


QComboBox#cboResolution QAbstractItemView {
    background-color: #1F2937; /* var(--gray-800) */
    color: #F3F4F6; /* var(--gray-100) */
    border: 1px solid #374151; /* var(--gray-700) */
    border-radius: 6px;
    padding: 4px;
    selection-background-color: #3B82F6; /* var(--primary) */
    selection-color: white;
}



/* QProgressBar 样式 */
QProgressBar#pbarDownload {
    background-color: #374151; /* var(--gray-700) */
    border-radius: 4px;
	color: #9CA3AF;  /* var(--gray-400) */
    text-align: right;
}

QProgressBar#pbarDownload::chunk {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                                stop:0 #10B981, stop:1 #059669); /* var(--success) to var(--success-dark) */
    border-radius: 4px;
}</string>
